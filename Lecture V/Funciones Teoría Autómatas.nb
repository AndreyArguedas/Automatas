   RowBox[{"NFAtoDFA", "[", 
    RowBox[{"\[Delta]_", ",", "nstates_", ",", "\[CapitalSigma]_"}], "]"}], ":=", 
   RowBox[{"AssociationThread", "[", "\[IndentingNewLine]", 
