          RowBox[{
           RowBox[{
            RowBox[{"\[Delta]", "[", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"#", "\[LeftDoubleBracket]", 
                RowBox[{"1", ",", "i"}], "\[RightDoubleBracket]"}], ",", 
               RowBox[{
               "#", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
              "}"}], "]"}], "/.", 
            RowBox[{"_Missing", "\[Rule]", "Nothing"}]}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", 
             RowBox[{"Length", "[", "#1", "]"}]}], "}"}]}], "]"}]}], "&"}], 
       ",", 
       RowBox[{"Flatten", "[", 
        RowBox[{
         RowBox[{"Outer", "[", 
          RowBox[{
