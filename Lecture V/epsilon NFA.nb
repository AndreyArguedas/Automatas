Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"\[Delta]", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"#1", "[", 
          RowBox[{"[", "i", "]"}], "]"}], ",", "\[Epsilon]"}], "}"}], "]"}], "/.", 
      RowBox[{"_Missing", "\[Rule]", 
       RowBox[{"{", "}"}]}]}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", 
       RowBox[{"Length", "[", "#1", "]"}]}], "}"}]}], "]"}], "&"}], "[", 
  RowBox[{"{", 
   StyleBox["0",
    FontColor->RGBColor[1, 0, 0]], "}"}], "]"}]], "Input",
