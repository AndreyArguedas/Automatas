   RowBox[{"\[Delta]_", ",", "state_List"}], "]"}], ":=", 
  RowBox[{"Union", "[", 
   RowBox[{
    RowBox[{"Map", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Flatten", "[", 
        RowBox[{"Most", "[", 
         RowBox[{
          RowBox[{"NestWhileList", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Union", "@@", 
              RowBox[{"Table", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"\[Delta]", "[", 
                  RowBox[{"{", 
                   RowBox[{
                    RowBox[{"#1", "[", 
                    RowBox[{"[", "i", "]"}], "]"}], ",", "\[Epsilon]"}], 
                   "}"}], "]"}], "/.", 
                 RowBox[{"_Missing", "\[Rule]", 
                  RowBox[{"{", "}"}]}]}], ",", 
                RowBox[{"{", 
                 RowBox[{"i", ",", 
