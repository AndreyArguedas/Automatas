Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"validateKeyRange", "[", 
     RowBox[{"alphabet_", ",", " ", "publicKey_"}], "]"}], " ", ":=", " ", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", "}"}], ",", "\n", "\t", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"publicKey", " ", ">", " ", 
         RowBox[{"Length", "[", "alphabet", "]"}]}], ",", " ", 
        RowBox[{"Mod", "[", 
         RowBox[{"publicKey", ",", 
          RowBox[{"Length", "[", "alphabet", "]"}]}], "]"}], ",", " ", 
        "publicKey"}], "]"}]}], "\n", "]"}]}], ";"}], "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"encryptorDecryptor", "[", 
     RowBox[{"alphabet_", ",", "message_", ",", "publicKey_"}], "]"}], ":=", 
    " ", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
       "chars", ",", " ", "alphabetLength", ",", " ", "currentPositions", ",",
         " ", "newPositions", ",", " ", "newMessage", ",", "validatedKey"}], 
       "}"}], ",", "\n", 
      RowBox[{
       RowBox[{"alphabetLength", " ", "=", " ", 
        RowBox[{"Length", "[", "alphabet", "]"}]}], ";", "\n", 
       RowBox[{"validatedKey", " ", "=", " ", 
        RowBox[{"validateKeyRange", "[", 
         RowBox[{"alphabet", ",", " ", "publicKey"}], "]"}]}], ";", "\n", 
       RowBox[{"chars", " ", "=", " ", 
        RowBox[{"Characters", "[", "message", "]"}]}], ";", "\n", 
       RowBox[{"currentPositions", " ", "=", " ", 
        RowBox[{
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"Position", "[", 
            RowBox[{"alphabet", ",", "i"}], "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", " ", "chars"}], "}"}]}], "]"}], " ", "//", 
         "Flatten"}]}], ";", "\n", 
       RowBox[{"newPositions", " ", "=", " ", 
        RowBox[{"Map", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"#", " ", "+", " ", "validatedKey"}], " ", ">", " ", 
              "alphabetLength"}], ",", " ", 
             RowBox[{
             "#", " ", "+", " ", "validatedKey", " ", "-", " ", 
              "alphabetLength"}], ",", " ", 
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{
                RowBox[{"#", " ", "+", " ", "validatedKey"}], " ", "<=", " ", 
                "0"}], ",", " ", 
               RowBox[{"alphabetLength", " ", "+", " ", 
                RowBox[{"(", 
                 RowBox[{"#", " ", "+", " ", "validatedKey"}], ")"}]}], ",", 
               RowBox[{"#", " ", "+", " ", "validatedKey"}]}], "]"}]}], "]"}],
            "&"}], ",", "currentPositions"}], "]"}]}], ";", "\n", 
       RowBox[{"newMessage", " ", "=", " ", 
        RowBox[{
         RowBox[{
          RowBox[{"FromLetterNumber", "[", "#", "]"}], "&"}], " ", "/@", " ", 
         "newPositions"}]}], ";", "\n", 
       RowBox[{"StringJoin", "[", "newMessage", "]"}]}]}], "\n", "]"}]}], 
   ";"}], "\n"}], "\n", 
