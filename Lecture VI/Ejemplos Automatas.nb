   3.838369505643907*^9, {3.838391081439848*^9, 3.8383911316164284`*^9}, {
   3.838537866620694*^9, 3.838537866621602*^9}, {3.8385382517173777`*^9, 
   3.8385382765326*^9}, 3.838623494825499*^9, {3.839014112170587*^9, 
   3.839014137388648*^9}, {3.839317153446558*^9, 3.8393171781221523`*^9}, {
   3.839345824407113*^9, 3.839345838654241*^9}, 3.839355377481254*^9},
 DefaultNaturalLanguage->
