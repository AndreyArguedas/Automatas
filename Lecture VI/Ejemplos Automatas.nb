   "]"}], ":=", 
  RowBox[{"Grid", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Join", "[", 
     RowBox[{
      RowBox[{"List", "/@", 
       RowBox[{"Prepend", "[", 
        RowBox[{"states", ",", "\"\<Q\\\[CapitalSigma]\>\""}], "]"}]}], ",", 
      RowBox[{"Prepend", "[", 
       RowBox[{
        RowBox[{"Partition", "[", 
         RowBox[{
          RowBox[{"Flatten", "[", 
           RowBox[{
            RowBox[{"Outer", "[", 
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"\[Delta]", "[", 
                 RowBox[{"{", 
                  RowBox[{"#1", ",", "#2"}], "}"}], "]"}], "/.", 
                RowBox[{"_Missing", "\[Rule]", "\"\<-\>\""}]}], "&"}], ",", 
              "states", ",", "\[CapitalSigma]", ",", "1"}], "]"}], ",", "1"}],
            "]"}], ",", 
