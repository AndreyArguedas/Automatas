Cell[BoxData[{
 RowBox[{
  RowBox[{"Closure", "[", 
   RowBox[{"\[Delta]_", ",", "state_List"}], "]"}], ":=", 
  RowBox[{"Union", "[", 
   RowBox[{
    RowBox[{"Map", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Flatten", "[", 
        RowBox[{"Most", "[", 
         RowBox[{
          RowBox[{"NestWhileList", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"Union", "@@", 
              RowBox[{"Table", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"\[Delta]", "[", 
                  RowBox[{"{", 
                   RowBox[{
                    RowBox[{"#1", "[", 
                    RowBox[{"[", "i", "]"}], "]"}], ",", "\[Epsilon]"}], 
                   "}"}], "]"}], "/.", 
                 RowBox[{"_Missing", "\[Rule]", 
                  RowBox[{"{", "}"}]}]}], ",", 
                RowBox[{"{", 
                 RowBox[{"i", ",", 
                  RowBox[{"Length", "[", "#1", "]"}]}], "}"}]}], "]"}]}], 
             "&"}], ",", 
            RowBox[{"{", "#", "}"}], ",", 
            RowBox[{
             RowBox[{"#", "\[NotEqual]", " ", 
              RowBox[{"{", "}"}]}], "&"}]}], "]"}], "/.", 
          RowBox[{"_Missing", "\[Rule]", 
           RowBox[{"{", "}"}]}]}], "]"}], "]"}], "&"}], ",", "state"}], "]"}],
     "//", "Flatten"}], "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"DeltaHat", "[", 
   RowBox[{"\[Delta]_", ",", "q0_List", ",", "string_"}], "]"}], ":=", 
  RowBox[{"FoldList", "[", 
   RowBox[{
    RowBox[{
     RowBox[{"Closure", "[", 
      RowBox[{"\[Delta]", ",", 
       RowBox[{"Union", "@@", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"\[Delta]", "[", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"#1", "[", 
               RowBox[{"[", "i", "]"}], "]"}], ",", "#2"}], "}"}], "]"}], "/.", 
           RowBox[{"_Missing", "\[Rule]", "Nothing"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", 
            RowBox[{"Length", "[", "#1", "]"}]}], "}"}]}], "]"}]}]}], "]"}], 
     "&"}], ",", 
    RowBox[{"Closure", "[", 
     RowBox[{"\[Delta]", ",", "q0"}], "]"}], ",", 
    RowBox[{"Characters", "[", "string", "]"}]}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"IsValidQ", "[", 
   RowBox[{"\[Delta]_", ",", "q0_List", ",", "qf_List", ",", "string_"}], 
   "]"}], ":=", 
  RowBox[{"ContainsAny", "[", 
   RowBox[{"qf", ",", 
    RowBox[{"Fold", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Closure", "[", 
        RowBox[{"\[Delta]", ",", 
         RowBox[{"Union", "@@", 
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"\[Delta]", "[", 
              RowBox[{"{", 
               RowBox[{
                RowBox[{"#1", "[", 
                 RowBox[{"[", "i", "]"}], "]"}], ",", "#2"}], "}"}], "]"}], "/.", 
             RowBox[{"_Missing", "\[Rule]", "Nothing"}]}], ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", 
              RowBox[{"Length", "[", "#1", "]"}]}], "}"}]}], "]"}]}]}], "]"}],
        "&"}], ",", 
      RowBox[{"Closure", "[", 
       RowBox[{"\[Delta]", ",", "q0"}], "]"}], ",", 
      RowBox[{"Characters", "[", "string", "]"}]}], "]"}]}], 
   "]"}]}]}], "Input",
