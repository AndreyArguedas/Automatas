Cell[BoxData[
 RowBox[{
  RowBox[{"DeltaHat", "[", 
   RowBox[{"{", 
    RowBox[{"state_", ",", "string_", ",", "stack_"}], "}"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"trans", ",", " ", 
      RowBox[{"tempstack", "=", "stack"}], ",", 
      RowBox[{"tempstring", "=", "string"}]}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"trans", "=", 
      RowBox[{"\[Delta]", "[", 
       RowBox[{"{", 
        RowBox[{"state", ",", 
         RowBox[{"First", "[", 
          RowBox[{"Pop", "[", "tempstring", "]"}], "]"}], ",", 
         RowBox[{"First", "[", 
          RowBox[{"Pop", "[", "tempstack", "]"}], "]"}]}], "}"}], "]"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"First", "[", "trans", "]"}], ",", "tempstring", ",", 
       RowBox[{"Push", "[", 
        RowBox[{"tempstack", ",", 
         RowBox[{"Last", "[", "trans", "]"}]}], "]"}]}], "}"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
